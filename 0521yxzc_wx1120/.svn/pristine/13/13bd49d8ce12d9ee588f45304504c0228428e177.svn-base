package com.plugs.module_order.pojo;

/**
 * OrderCostItem 费用明细项目
 *
 * @author Zoro
 * @date 2016/11/29
 */
public class OrderCostItem {

    private String item;//费用项目
    private String cost;//费用
    private Integer sort;//排序

    public OrderCostItem() {
    }

    public OrderCostItem(String item, String cost, Integer sort) {
        this.item = item;
        this.cost = cost;
        this.sort = sort;
    }

    public String getItem() {
        return item;
    }

    public void setItem(String item) {
        this.item = item;
    }

    public String getCost() {
        return cost;
    }

    public void setCost(String cost) {
        this.cost = cost;
    }

    public Integer getSort() {
        return sort;
    }

    public void setSort(Integer sort) {
        this.sort = sort;
    }
}
