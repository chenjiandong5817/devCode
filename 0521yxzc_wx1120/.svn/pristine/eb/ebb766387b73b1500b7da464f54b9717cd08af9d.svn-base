package com.plugs.module_order.dtos;

import java.util.Date;

/**
 * 订单评价实体
 *
 * @author Zoro
 * @since 2016/09/28
 */
public class OrderEvaluateDto {
    private String uuid;        //主键
    private String orderUuid;        //
    private String passengerUuid;        //
    private String passengerMobile;        //电话
    private String passengerName;        //名称
    private String driverUuid;        //司机UUID
    private double evaluateScore;        //评分
    private String evaluateContent;        //评价内容
    private String evaluateTag;        //评价标签
    private Date createTime;        //创建时间
    private String creator;        //创建者

    public void setUuid(String uuid) {
        this.uuid = uuid;
    }

    public String getUuid() {
        return uuid;
    }

    public String getOrderUuid() { return orderUuid; }

    public void setOrderUuid(String orderUuid) { this.orderUuid = orderUuid; }

    public void setPassengerUuid(String passengerUuid) {
        this.passengerUuid = passengerUuid;
    }

    public String getPassengerUuid() {
        return passengerUuid;
    }

    public void setPassengerMobile(String passengerMobile) {
        this.passengerMobile = passengerMobile;
    }

    public String getPassengerMobile() {
        return passengerMobile;
    }

    public void setPassengerName(String passengerName) {
        this.passengerName = passengerName;
    }

    public String getPassengerName() { return passengerName; }

    public String getDriverUuid() { return driverUuid; }

    public void setDriverUuid(String driverUuid) { this.driverUuid = driverUuid; }

    public void setEvaluateScore(double evaluateScore) {
        this.evaluateScore = evaluateScore;
    }

    public double getEvaluateScore() {
        return evaluateScore;
    }

    public void setEvaluateContent(String evaluateContent) {
        this.evaluateContent = evaluateContent;
    }

    public String getEvaluateContent() {
        return evaluateContent;
    }

    public void setEvaluateTag(String evaluateTag) {
        this.evaluateTag = evaluateTag;
    }

    public String getEvaluateTag() {
        return evaluateTag;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreator(String creator) {
        this.creator = creator;
    }

    public String getCreator() {
        return creator;
    }

}
